#!/bin/bash

source ./paths.sh
rm -rf $backupdir

for i in "${paths[@]}"
do
    dir=$(dirname $i)
    mkdir -p $backupdir/$dir
    cp -r ~/$i $backupdir/$dir
done

find ./files -type f -name '*history*' -exec rm {} +
find ./files -type f -name '*bookmark*' -exec rm {} +
find ./files -type f -name '*tagged*' -exec rm {} +
find ./files -type f -name '*cache*' -exec rm {} +
